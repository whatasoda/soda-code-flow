{
  "name": "soda-code-flow",
  "version": "1.0.0",
  "description": "ゼミの第一課題で制作する予定の「プログラミングのコードの流れを可視化するもの」のためのrepo。babel-plugin等も全部ここで管理する",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development -w -r ts-node/register",
    "build": "npm run clean && npm run build:app && npm run build:functions && npm run build:deps",
    "build:app": "NODE_ENV=production webpack --mode production -r ts-node/register",
    "build:functions": "tsc -p tsconfig.functions.json",
    "build:deps": "npm run build-deps:package.json && npm run build-deps:package-lock.json && npm run build-deps:install",
    "build-deps:package.json": "cat package.json | jq \". | { name: .name, dependencies: .dependencies, repository: .repository, author: .author, private: .private, engines: .engines }\" > dist/functions/package.json",
    "build-deps:package-lock.json": "cpx package-lock.json dist/functions",
    "build-deps:install": "npm --prefix dist/functions i dist/functions",
    "clean": "rimraf dist/functions && rimraf dist/app ",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "check": "tslint -p tsconfig.json && prettier -l **/*.{ts,tsx}",
    "fix": "tslint --fix -p tsconfig.json && prettier --write **/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whatasoda/soda-code-flow.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whatasoda/soda-code-flow/issues"
  },
  "homepage": "https://github.com/whatasoda/soda-code-flow#readme",
  "devDependencies": {
    "@types/babel__core": "^7.0.1",
    "@types/babel__generator": "^7.0.0",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__template": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.32",
    "@types/koa-static": "^4.0.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/pako": "^1.0.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-highlight": "^0.12.1",
    "@types/react-redux": "^6.0.9",
    "@types/uglify-es": "^3.0.0",
    "@types/webpack": "^4.4.17",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.18.0",
    "copy-webpack-plugin": "^4.5.4",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.5",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "highlightjs": "^9.12.0",
    "husky": "^1.1.2",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.5.2",
    "react-highlight": "^0.12.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "rimraf": "^2.6.2",
    "sanitize.css": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/core": "^7.1.5",
    "@babel/generator": "^7.1.3",
    "@babel/parser": "^7.1.3",
    "@babel/traverse": "^7.1.4",
    "@babel/types": "^7.1.3",
    "firebase-admin": "~6.0.0",
    "firebase-functions": "^2.0.3",
    "pako": "^1.0.6",
    "uglify-es": "^3.3.9"
  },
  "engines": {
    "node": "8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browser": {
    "fs": false
  }
}
